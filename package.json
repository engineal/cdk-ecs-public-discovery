{
  "name": "cdk-ecs-public-discovery",
  "version": "1.0.1",
  "description": "An AWS CDK construct library that provides ECS service discovery functionality for an ECS task's public IP address",
  "author": "Aaron Lucia",
  "license": "Apache-2.0",
  "homepage": "https://constructs.dev/packages/cdk-ecs-public-discovery",
  "repository": {
    "type": "git",
    "url": "https://github.com/engineal/cdk-ecs-public-discovery.git"
  },
  "keywords": [
    "cdk",
    "constructs",
    "aws",
    "ecs",
    "servicediscovery",
    "lambda"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "is-ci || husky install",
    "lint": "eslint .",
    "build": "jsii",
    "watch": "jsii --watch",
    "package": "jsii-pacmak",
    "release": "jsii-release",
    "test": "jest"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/aws-lambda": "^8.10.110",
    "@types/jest": "^27.5.2",
    "@types/node": "18.11.19",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "aws-cdk-lib": "2.65.0",
    "constructs": "10.0.0",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^27.5.1",
    "jsii": "^1.75.0",
    "jsii-pacmak": "^1.75.0",
    "jsii-release": "^0.2.598",
    "mocked-env": "^1.3.5",
    "semantic-release": "^20.1.0",
    "ts-jest": "^27.1.5",
    "typescript": "~4.9.5"
  },
  "peerDependencies": {
    "aws-cdk-lib": "2.65.0",
    "constructs": "^10.0.0"
  },
  "bundledDependencies": [
    "aws-sdk",
    "aws-xray-sdk-core"
  ],
  "dependencies": {
    "aws-sdk": "^2.1326.0",
    "aws-xray-sdk-core": "^3.4.1"
  },
  "stability": "experimental",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "full",
    "targets": {
      "java": {
        "package": "com.engineal.cdk.ecspublicdiscovery",
        "maven": {
          "groupId": "com.engineal.cdk",
          "artifactId": "ecs-public-discovery"
        }
      },
      "python": {
        "distName": "cdk-ecs-public-discovery",
        "module": "cdk_ecs_public_discovery"
      },
      "dotnet": {
        "namespace": "EngineAL.CDK.EcsPublicDiscovery",
        "packageId": "EngineAL.CDK.EcsPublicDiscovery"
      }
    }
  }
}
